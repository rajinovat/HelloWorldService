<project name="IIB-CI-MAIN" basedir="..">
  <description>IIB Continuous Integration(main script)</description>
<property name="version" value="${bar.version}" />  
    

    <exec executable="mqsicreatebar">
      <arg value="-data" />
      <arg value="${basedir}" />
      <arg value="-b" />
